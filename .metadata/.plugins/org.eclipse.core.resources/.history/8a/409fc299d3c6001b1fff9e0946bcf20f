package com.db.recruit;

public class RecruitDto {
	private String img;
	private String comName;
	private String tel;
	private String fax;
	private String address;
	private int no;
	private String title;
	private String[] tag;
	private String[] isCareer;
	private String category;
	private String text1;
	private String text2;
	private String text3;
	private String text4;
	private String text5;
	
	public RecruitDto() {}
	public RecruitDto(String img, String comName, String tel, String fax, String address, int no, String title,
			String[] tag, String[] isCareer, String category, String text1, String text2, String text3, String text4,
			String text5) {
		super();
		this.img = img;
		this.comName = comName;
		this.tel = tel;
		this.fax = fax;
		this.address = address;
		this.no = no;
		this.title = title;
		this.tag = tag;
		this.isCareer = isCareer;
		this.category = category;
		this.text1 = text1;
		this.text2 = text2;
		this.text3 = text3;
		this.text4 = text4;
		this.text5 = text5;
	}
	
}
